<h1>This is the list of signers in the  selected city</h1>

<h2>Total signers: {{length}}</h2>

<ul>
    {{#each content}}
{{#if this.url}}
<li><a href="{{this.url}}">{{this.first_name}} {{this.last_name}}</a>  {{this.age}}</li>
	{{else}}
<li>{{this.first_name}} {{this.last_name}} {{this.age}}</li>
	{{/if}}

{{/each}}
</ul>

{{>logout}}
