<h1>This is the list of signers</h1>

<h2>Total signers: {{length}}</h2>

    <ul>
{{#each content}}
{{#if url}}
{{#if city}}
    <li><a href="{{this.url}}">{{this.first_name}} {{this.last_name}}</a>  {{this.age}} <a href="{{this.city}}">{{this.city}}</a></li>
{{/if}}
{{/if}}
{{else}}
<li>{{this.first_name}} {{this.last_name}}  {{this.age}} {{this.city}} {{this.url}}</li>
{{/each}}

</ul>
